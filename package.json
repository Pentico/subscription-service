{
  "name": "subscription-service",
  "version": "1.0.0",
  "description": "REST API service managing subscription pricing, services, consumables, VAT, etc. Built in Node.js.",
  "author": "Weld (www.weld.io)",
  "license": "MIT",
  "main": "app/server.js",
  "scripts": {
    "dev": "nodemon app/server.js",
    "start": "node app/server.js",
    "testall": "echo 'Running Standard.js and Tape API tests...\n' && yarn lint && yarn unit",
    "test": "export DISABLE_JWT=true && node test/users.js && node test/subscriptions.js && node test/app.js",
    "test-tape": "DISABLE_JWT=true tape test/**/*.js",
    "lint": "standard",
    "fix": "standard --fix"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
    ],
    "globals": [
    ]
  },
  "engines": {
    "node": "^8.1.0",
    "yarn": "^1.0.1"
  },
  "dependencies": {
    "async": "^2.5.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cors": "^2.8.4",
    "express": "^4.15.2",
    "express-jwt": "^5.3.0",
    "glob": "^7.1.1",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.29",
    "mongoose": "^4.10.8",
    "mongoose-crudify": "git+https://github.com/ryo718/mongoose-crudify.git#0.2.0",
    "morgan": "^1.8.2",
    "node-fetch": "^1.7.3",
    "resource-router-middleware": "^0.7.0",
    "stripe": "^5.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "nodemon": "^1.11.0",
    "standard": "^12.0.1",
    "supertest": "^3.0.0",
    "tape": "^4.6.3"
  }
